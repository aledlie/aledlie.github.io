<file path="sumedhjoshi.github.io/_sass/elements.scss">
@use 'mixins';
@use 'variables';
@use 'grid';

/* 
   Rules
   ========================================================================== */

hr { 
	display: block; 
	margin: 1em 0; 
	padding: 0;
	height: 1px; 
	border: 0; 
	border-top: 1px solid #ccc;
	border-bottom: 1px solid #fff; 
}

/* 
   Figures and images
   ========================================================================== */

figure {
	margin-bottom: mixins.$indent-var;
}

article img {
	max-width: 100%;
	height: auto;
	border-width: 0; 
	vertical-align: middle; 
	-ms-interpolation-mode: bicubic; 
}

svg:not(:root) {
		overflow: hidden; 
}

/* 2 images of equal dimensions in a row */

.half {
	@include grid.container;
	@include mixins.clearfix;
	@media #{variables.$micro} {
		img {
			@include grid.grid(12,6);
		}

		figcaption {
			@include grid.grid(12,12);
		}
	}
}

/* 3 images of equal dimensions in a row */

.third {
	@include grid.container;
	@include mixins.clearfix;
	@media #{variables.$micro} {
		img {
			@include grid.grid(12,4);
		}

		figcaption {
			@include grid.grid(12,12);
		}
	}
}

/* 
   Buttons
   ========================================================================== */

/* Default button */

.btn {
  display: inline-block;
  margin-bottom: 20px;
  padding: 8px 20px;
  @include mixins.font-rem(14);

  font-family: variables.$heading-font;
  font-weight: 700;
  background-color: variables.$primary;
  color: variables.$white;
  border-width: 2px !important;
  border-style: solid !important;
  border-color: variables.$primary;
  text-decoration: none !important;
  @include mixins.rounded(3px);

  &:visited {
    color: variables.$white;
  }

  &:hover {
    background-color: variables.$white;
    color: variables.$primary;
  }
}

/* Success button */

.btn-success {
  background-color: variables.$success;
  color: variables.$white;
  border-color: variables.$success;

  &:visited {
    color: variables.$white;
  }

  &:hover {
    background-color: variables.$white;
    color: variables.$success;
  }
}

/* Warning button */

.btn-warning {
  background-color: variables.$warning;
  color: variables.$white;
  border-color: variables.$warning;

  &:visited {
    color: variables.$white;
  }

  &:hover {
    background-color: variables.$white;
    color: variables.$warning;
  }
}

/* Danger button */

.btn-danger {
  background-color: variables.$danger;
  color: variables.$white;
  border-color: variables.$danger;

  &:visited {
    color: variables.$white;
  }

  &:hover {
    background-color: variables.$white;
    color: variables.$danger;
  }
}

/* Information button */

.btn-info {
  background-color: variables.$info;
  color: variables.$white;
  border-color: variables.$info;

  &:visited {
    color: variables.$white;
  }

  &:hover {
    background-color: variables.$white;
    color: variables.$info;
  }
}

/* 
   Wells
   ========================================================================== */

.well {
	min-height: 20px;
	padding: 19px;
	margin-bottom: 20px;
	background-color: #f5f5f5;
	border: 1px solid #e3e3e3;
	@include mixins.rounded(4px);
	@include mixins.box-shadow(inset 0 1px 1px rgba(0, 0, 0, 0.05));
}
</file>

<file path="sumedhjoshi.github.io/_sass/forms.scss">
@use "sass:color";
@use 'variables' as *;
@use 'mixins';

/* ==========================================================================
   Forms
   ========================================================================== */

btnform { 
	margin: 0 0 5px;

	fieldset { 
		margin-bottom: 5px; 
		padding: 0; 
		border-width: 0; 
	}

	legend { 
		display: block;
		width: 100%;
		margin-bottom: 5px * 2;
		*margin-left: -7px; 
		padding: 0;
		color: $textcolor;
		border: 0;
		border-bottom: 1px solid color.adjust($black, $lightness: 80%);
		white-space: normal; 
	}

	p {
		margin-bottom: 5px / 2; 
	}

	ul {
		list-style-type: none; 
		margin: 0 0 5px;
		padding: 0; 
	}

	br {
		display: none; 
	}
}

label,
input,
button,
select,
textarea {
	vertical-align: baseline; 
	*vertical-align: middle; 
}

input,
button,
select,
textarea {
	font-family: $base-font; 
	@include mixins.box-sizing(border-box); 
}

label {
	display: block;
	margin-bottom: 10px / 8;
	font-weight: bold;
	color: $textcolor;
	cursor: pointer; 

	input,
	textarea,
	select {
		display: block; 
	}
}

input,
textarea,
select {
	display: inline-block;
	width: 100%;
	padding: 4px;
	margin-bottom: 5px / 4;
	background-color: $white;
	border: 1px solid color.adjust($black, $lightness: 80%);
	color: $textcolor; 

	&:hover {
		border-color: color.adjust($accentcolor, $lightness: 50%); 
	}
}

.input-mini { 
	width: 60px; 
}

.input-small { 
	width: 90px; 
}

input[type="image"],
input[type="checkbox"],
input[type="radio"] {
	width: auto;
	height: auto;
	padding: 0;
	margin: 3px 0;
	*margin-top: 0;
	line-height: normal;
	cursor: pointer;
	@include mixins.rounded(0);

	border: 0 \9; 
}

input[type="checkbox"], 
input[type="radio"] {  
	@include mixins.box-sizing(border-box);

	padding: 0; 
	*width: 13px; 
	*height: 13px; 
}

input[type="image"] {
	border: 0; 
	@include mixins.box-shadow(none); 
}

input[type="file"] {
	width: auto;
	padding: initial;
	line-height: initial;
	border: initial;
	background-color: transparent;
	background-color: initial;
	@include mixins.box-shadow(none); 
}

input[type="button"],
input[type="reset"],
input[type="submit"] {
	width : auto;
	height : auto;
	cursor : pointer;
	*overflow : visible; 
}

select,
input[type="file"] {
	*margin-top : 4px;  
}

select {
	width : auto;
	background-color : $white;
}

select[multiple],
select[size] {
	height : auto; 
}

textarea {
	@include mixins.resize(vertical);

	height : auto;
	overflow : auto; 
	vertical-align : top; 
}

input[type="hidden"] {
	display : none; 
}

.radio,
.checkbox {
	padding-left : 18px; 
	font-weight : normal; 
}

.radio input[type="radio"],
.checkbox input[type="checkbox"] {
	float : left;
	margin-left : -18px; 
}

.radio.inline,
.checkbox.inline {
	display : inline-block;
	padding-top : 5px;
	margin-bottom : 0;
	vertical-align : middle; 
}

.radio.inline + .radio.inline,
.checkbox.inline + .checkbox.inline {
	margin-left : 10px; 
}

/* 
   Disabled state
   ========================================================================== */

input[disabled],
select[disabled],
textarea[disabled],
input[readonly],
select[readonly],
textarea[readonly] {
	@include mixins.opacity(.5);

	cursor : not-allowed; 
}

/* 
   Focus & active state
   ========================================================================== */

input:focus,
textarea:focus {
	border-color : $accentcolor;
	outline : 0;
	outline : thin dotted \9; 
}

input[type="file"]:focus,
input[type="radio"]:focus,
input[type="checkbox"]:focus,
select:focus {
	@include mixins.box-shadow(none); 
}

/* 
   Help text
   ========================================================================== */

.help-block,
.help-inline {
	color : color.adjust($black, $lightness: 50%); 
}

.help-block {
	display : block; 
	margin-bottom : 1em; 
	line-height : 1em;
}

.help-inline {
	display : inline-block;
	vertical-align : middle;
	padding-left : 5px; 
}

/* 
   .form-inline
   ========================================================================== */

.form-inline input,
.form-inline textarea,
.form-inline select {
	display : inline-block;
	margin-bottom : 0; 
}

.form-inline label {
	display : inline-block; 
}

.form-inline .radio,
.form-inline .checkbox,
.form-inline .radio {
	padding-left : 0;
	margin-bottom : 0;
	vertical-align : middle; 
}

.form-inline .radio input[type="radio"],
.form-inline .checkbox input[type="checkbox"] {
	float : left;
	margin-left : 0;
	margin-right : 3px; }

/* 
   .form-search
   ========================================================================== */

.form-search input,
.form-search textarea,
.form-search select {
	display : inline-block;
	margin-bottom : 0; 
}

.form-search .search-query {
	padding-left : 14px;
	padding-right : 14px;
	margin-bottom : 0; 
	@include mixins.rounded(14px); 
}

.form-search label {
	display : inline-block;
}

.form-search .radio,
.form-search .checkbox,
.form-inline .radio {
	padding-left : 0;
	margin-bottom : 0;
	vertical-align : middle;
}

.form-search .radio input[type="radio"],
.form-search .checkbox input[type="checkbox"] {
	float : left;
	margin-left : 0;
	margin-right : 3px;
}
