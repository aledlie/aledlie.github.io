t\th1, h2 {\n\t\t\tcolor: variables.$white;\n\t\t\t@include mixins.text-shadow(0 1px 2px rgba(0,0,0,.25));\n\t\t}\n\t}\n}\n\n/* Post byline */\n.byline {\n\tclear: both;\n\tfont-size: 80%;\n}\n.article-author-side,\n.article-author-bottom {\n\th3 {\n\t\tmargin-bottom: 0;\n\t}\n\tp {\n\t\tfont-size: 80%;\n\t\tfont-style: italic;\n\t}\n\ta, a:hover {\n\t\tborder-bottom: 0 solid transparent;\n\t}\n}\n\n/* Default social media links in author sidebar */\n.author-social {\n  display: block;\n  margin-bottom: 5px;\n  @include mixins.font-rem(14);\n  color: variables.$black;\n  &:visited {\n    color: variables.$black;\n  }\n  &:hover {\n    @include mixins.scale(1.1);\n  }\n  &:active {\n    @include mixins.translate(0, 2px);\n  }\n  .fa {\n\t\tmargin-right: 5px;\n\t}\n}\n\n/* Author sidebar */\n.article-author-side {\n\tdisplay: none;\n\t.bio-photo {\n\t\tmax-width: 110px;\n\t\t@include mixins.rounded(150px);\n\t}\n\t@media #{variables.$small} {\n\t\tdisplay: block;\n\t\t@include grid.grid(12,2);\n\t\t@include grid.prefix(12,0.5);\n\t\t@include grid.suffix(12,0.5);\n\t\th3,\n\t\tp,\n\t\t.author-social {\n\t\t\tdisplay: block;\n\t\t\tmax-width: 125px;\n\t\t}\n\t\th3 {\n\t\t\tmargin-top: 10px;\n\t\t\tmargin-bottom: 10px;\n\t\t}\n\t\tp {\n\t\t\tmargin-bottom: 20px;\n\t\t}\n\t\t.author-social {\n\t\t\tmargin-bottom: 5px;\n\t\t}\n\t}\n\t@media #{variables.$large} {\n\t\th3,\n\t\t.bio-photo,\n\t\tp,\n\t\t.author-social {\n\t\t\tmax-width: 150px;\n\t\t}\n\t}\n\t@media #{variables.$x-large} {\n\t\t@include grid.grid(12,1.5);\n\t\t@include grid.prefix(12,2);\n\t}\n}\n\n/* Author module - mobile only */\n.article-author-bottom {\n\tmargin-bottom: 1em;\n\t@media #{variables.$small} {\n\t\tdisplay: none;\n\t}\n\t.bio-photo {\n\t\tfloat: left;\n\t\tmargin-right: 25px;\n\t\tmax-width: 100px;\n\t\t@include mixins.rounded(150px);\n\t}\n\t.author-social {\n\t\tdisplay: inline-block;\n\t\tmargin-right: 10px;\n\t}\n\t@media #{variables.$large} {\n\t\th3,\n\t\t.bio-photo,\n\t\tp,\n\t\t.author-social {\n\t\t\tmax-width: 150px;\n\t\t}\n\t}\n}\n\n/* Post content wrapper */\n.article-wrap {\n\t// Dotted line underlines for links\n\tp > a,\n\tli > a {\n\t\ttext-decoration: underline;\n\t}\n}\n\n/* Table of contents */\n.toc {\n\tfont-size: 95%;\n\t@media #{variables.$large} {\n\t\tdisplay: block;\n\t\t@include grid.grid(12,2);\n\t\t@include grid.prefix(12,0.5);\n\t\t@include grid.suffix(12,0.5);\n\t\tposition: absolute;\n\t\ttop: 5.5em;\n\t\tright: 0;\n\t\tbackground-color: variables.$white;\n\t}\n\theader {\n\t\tbackground: color.adjust(variables.$black, $lightness: 10%);\n\t}\n\th3 {\n\t\tmargin: 0;\n\t\tpadding: 5px 10px;\n\t\tcolor: variables.$white;\n\t\t@include mixins.font-rem(16);\n\t\ttext-transform: uppercase;\n\t\t&:hover {\n\t\t\tcursor: pointer;\n\t\t}\n\t}\n\tul {\n\t\tmargin: 2px 0 0;\n\t\tpadding: 0;\n\t\tline-height: 1;\n\t}\n\tli {\n\t\tdisplay: block;\n\t\tmargin: 0 0 1px 0;\n\t\tpadding: 0;\n\t\tfont-family: variables.$heading-font;\n\t\tlist-style-type: none;\n\t\t&:last-child {\n\t\t\tborder-bottom-width: 0;\n\t\t}\n\t\ta {\n\t\t\tpadding: 10px;\n\t\t\tdisplay: block;\n\t\t\tcolor: variables.$white;\n\t\t\ttext-decoration: none;\n\t\t\tbackground: color.adjust(variables.$black, $lightness: 30%);\n\t\t\t@include mixins.opacity(0.7);\n\t\t\t@include mixins.transition(opacity 0.2s ease-in-out);\n\t\t\t&:hover {\n\t\t\t\t@include mixins.opacity(1);\n\t\t\t}\n\t\t}\n\t\tul {\n\t\t\tmargin: 1px 0 0;\n\t\t\tli a {\n\t\t\t\tpadding-left: 20px;\n\t\t\t}\n\t\t}\n\t}\n}\n\n/* TOC trigger for collapsing */\n#drawer {\n\tmax-height: 100%;\n\toverflow: hidden;\n\t&.js-hidden {\n\t\tmax-height: 0;\n\t}\n}\n\n/* Image grid - not used */\n.image-grid {\n\t@include mixins.clearfix;\n\tlist-style: none;\n\tmargin: 0 0 1em;\n\tpadding: 0;\n\tli {\n\t\t@include grid.grid(12,6);\n\t\t@media #{variables.$micro} {\n\t\t\twidth: 33.333333%;\n\t\t}\n\t\t@media #{variables.$small} {\n\t\t\twidth: 25%;\n\t\t}\n\t\t@media #{variables.$medium} {\n\t\t\twidth: 20%;\n\t\t}\n\t\t@media #{variables.$large} {\n\t\t\twidth: 16.666666666%;\n\t\t}\n\t}\n}\n\n/* Recent grid - not used */\n.recent-grid {\n\t@include mixins.clearfix;\n\tlist-style: none;\n\tmargin: 1em 0;\n\tli {\n\t\tdisplay: inline;\n\t\ta { \n\t\t\tborder-bottom: 0 solid transparent;\n\t\t\t&:hover {\n\t\t\t\tborder-bottom: 0 solid transparent;\n\t\t\t}\n\t\t}\n\t}\n\timg {\n\t\twidth: 19%;\n\t\tmargin-bottom: 1%;\n\t}\n}\n\n/* Social sharing links */\n/* Social media brand buttons */\n.social-share {\n  margin-bottom: 0px + variables.$doc-line-height;\n  margin-bottom: 0rem + math.div(variables.$doc-line-height, variables.$doc-font-size);\n  ul, li {\n  \tmargin: 0;\n  \tpadding: 0;\n  \tlist-style: none;\n  }\n  li {\n  \tdisplay: inline-block;\n  }\n  $social: \n  (facebook, variables.$facebook-color),\n  (flickr, variables.$flickr-color),\n  (foursquare, variables.$foursquare-color),\n  (google-plus, variables.$google-plus-color),\n  (instagram, variables.$instagram-color),\n  (linkedin, variables.$linkedin-color),\n  (pinterest, variables.$pinterest-color),\n  (rss, variables.$rss-color),\n  (tumblr, variables.$tumblr-color),\n  (twitter, variables.$twitter-color),\n  (vimeo, variables.$vimeo-color),\n  (youtube, variables.$youtube-color);\n  @each $socialnetwork, $color in $social {\n    .#{$socialnetwork} {\n      background: $color;\n    }    \n  }\n  a {\n\t  display: block;\n\t  padding: 8px 20px;\n\t  text-decoration: none !important;\n\t  text-transform: uppercase;\n\t  @include mixins.font-rem(14);\n\t  font-family: variables.$heading-font;\n\t  font-weight: 700;\n\t  color: variables.$white;\n\t  opacity: 0.8;\n\t  &:hover {\n\t    opacity: 1;\n\t  }\n\t}\n\tspan {\n\t\tdisplay: none;\n\t\t@media #{variables.$medium} {\n\t\t\tdisplay: inline;\n\t\t\tpadding-left: 5px;\n\t\t}\n\t}\n\th4 {\n\t\t@include mixins.font-rem(14);\n\t\tmargin-bottom: 10px;\n\t\ttext-transform: uppercase;\n\t}\n}\n\n\n/* Footer wrapper */\n.footer-wrap {\n\t@include grid.container;\n\t@include mixins.clearfix;\n\tclear: both;\n\tpadding-bottom: 3em;\n\ta,\n\ta:active,\n\ta:visited,\n\tp,\n\th4,\n\th5,\n\th6,\n\tspan { \n\t\t@include mixins.font-rem(14);\n\t}\n\tfooter {\n\t\t@include grid.grid(12,10);\n\t\t@include grid.prefix(12,1);\n\t\t@include grid.suffix(12,1);\n\t\t@media #{variables.$small} {\n\t\t\t@include grid.grid(12,6);\n\t\t\t@include grid.prefix(12,3);\n\t\t\t@include grid.suffix(12,3);\n\t\t}\n\t\t@media #{variables.$x-large} {\n\t\t\t@include grid.grid(12,4.5);\n\t\t\t@include grid.prefix(12,4);\n\t\t\t@include grid.suffix(12,3.5);\n\t\t}\n\t}\n}\n\n/* Related articles list */\n.related-articles {\n\t@include grid.grid(12,10);\n\t@include grid.prefix(12,1);\n\t@include grid.suffix(12,1);\n\tmargin-bottom: 2em;\n\t@media #{variables.$small} {\n\t\t@include grid.grid(12,6);\n\t\t@include grid.prefix(12,3);\n\t\t@include grid.suffix(12,3);\n\t}\n\t@media #{variables.$x-large} {\n\t\t@include grid.grid(12,4.5);\n\t\t@include grid.prefix(12,4);\n\t\t@include grid.suffix(12,3.5);\n\t}\n\th4 {\n\t\ttext-transform: uppercase;\n\t\tmargin-bottom: 0;\n\t}\n\tli {\n\t\tmargin-bottom: 0;\n\t}\n}\n\n/* \n   Browser upgrade alert\n   ========================================================================== */\n\n.browser-upgrade {\n\tbackground: #000;\n\ttext-align: center;\n\tmargin: 0 0 2em 0;\n\tpadding: 10px;\n\ttext-align: center;\n\tcolor: variables.$white;\n\ta {\n\t\tcolor: variables.$white;\n\t\tborder-bottom: 1px dotted variables.$white;\n\t\ttext-decoration: none;\n\t\t&:hover {\n\t\t\tborder-bottom: 1px solid variables.$white;\n\t\t}\n\t}\n}\n/* \n   Google search form\n   ========================================================================== */\n\n#goog-fixurl {\n\tul {\n\t\tlist-style: none;\n\t\tmargin-left: 0;\n\t\tpadding-left: 0;\n\t\tli {\n\t\t\tlist-style-type: none;\n\t\t}\n\t}\n}\n#goog-wm-qt {\n\twidth: auto;\n\tmargin-right: 10px;\n\tmargin-bottom: 20px;\n\tpadding: 8px 20px;\n\tdisplay: inline-block;\n\t@include mixins.font-rem(14);\n\tbackground-color: variables.$white;\n\tcolor: variables.$black;\n\tborder-width: 2px !important;\n\tborder-style: solid !important;\n\tborder-color: color.adjust(variables.$black,$lightness: 50%);\n\t@include mixins.rounded(3px);\n}\n#goog-wm-sb {\n\t@extend .btn;\n}\n"],"file":"main.css"}
