<file path="sumedhjoshi.github.io/_sass/site.scss">
@use "sass:color";
@use 'variables' as *;
@use 'mixins' as *;

/* ==========================================================================
   Site wide styles
   ========================================================================== */

/* 
   Selection
   ========================================================================== */

::selection { 
	background-color: color.adjust($basecolor, $lightness: 65%); 
	color: $basecolor; 
	text-shadow: none; 
}  	

::selection { 
	background-color: color.adjust($basecolor, $lightness: 65%); 
	color: $basecolor; 
	text-shadow: none; 
}

/* 
   Global classes
   ========================================================================== */

/* Capitalize */
.all-caps {
	text-transform: uppercase;
}

/* Float left */
.pull-left {
	float: left;
}

/* Float right */
.pull-right {
	float: right;
}

.image-pull-right {
	float: right;
	margin-top: 0;
}

/* Clearfix */
.clearfix {
	*zoom: 1;

	&::before,
	&::after {
		display: table;
	    content: "";
	}

	&::after {
		clear: both;
	}
}

/* Remove bullets and indentation from list */
.unstyled-list {
	list-style: none;
	margin-left: 0;
	padding-left: 0;

	li {
		list-style-type: none;
	}
}

/* 
   Global transition
   ========================================================================== */

b, i, strong, em, blockquote, p, q, span, figure, img, h1, h2, header, input, a {
	@include transition(all .2s ease);
}
</file>

<file path="sumedhjoshi.github.io/_sass/typography.scss">
@use "sass:color";
@use "variables";
@use "mixins";
@use "grid";

/* ==========================================================================
   Typography
   ========================================================================== */

/* 
   Headings
   ========================================================================== */

h1, h2, h3, h4, h5, h6 {
	font-family: variables.$heading-font;
}

h1 {
	@include mixins.font-size(32);
}

h2 {
	@include mixins.font-size(28);
}

h3 {
	@include mixins.font-size(24);
}

h4 {
	@include mixins.font-size(18);
}

h5 {
	@include mixins.font-size(16);
}

h6 {
	@include mixins.font-size(14);
}

/* 
   Links
   ========================================================================== */

a { 
	text-decoration: none;
	color: variables.$link-color; 

	&:visited { 
		color: variables.$linkcolorvisited; 
	}

	&:hover { 
		color: variables.$linkcolorhover; 
	}

	&:focus { 
		outline: thin dotted; 
		color: variables.$linkcolorfocus;
	}

	&:hover, 
	&:active { 
		outline: 0; 
	}
}

/* 
   Figure captions
   ========================================================================== */

figcaption {
	padding-top: 10px;
	@include mixins.font(14);

	line-height: 1.3;
	color: color.adjust(variables.$text-color, $lightness: 10%);
}

/* 
   Feature image captions
   ========================================================================== */

.image-credit {
	@include grid.grid(12,10);
	@include grid.prefix(12,1);
	@include grid.suffix(12,1);

	display: block;
	text-align: right;
	@include mixins.font(12);

	line-height: 1.3;
	font-style: italic;
	color: color.adjust(variables.$text-color, $lightness: 30%);
	@media #{variables.$small} {
		@include grid.grid(12,9);
		@include grid.prefix(12,0);
		@include grid.suffix(12,0);
	}
	@media #{variables.$x-large} {
		@include grid.grid(12,8.5);
	}

	a {
		color: color.adjust(variables.$text-color, $lightness: 30%);
	}
}

/* 
   Notices
   ========================================================================== */

.notice {
	margin-top: 1.5em;
	padding: .5em 1em;
	text-indent: 0;
	@include mixins.font-rem(14);

	background-color: variables.$body-color;
	border: 1px solid color.adjust(variables.$body-color,$lightness: -20%);
	@include mixins.rounded(3px);
}

/* 
   Blockquotes
   ========================================================================== */

blockquote {
	margin-left: -28px;
	padding-left: 20px;
	border-left: 8px solid variables.$border-color;
	font-family: variables.$alt-font;
	font-style: italic;
}

/* 
   Footnotes
   ========================================================================== */

.footnotes {
	ol, li, p {
		margin-bottom: 0;
		@include mixins.font-rem(14);
	}
}

/* 
   Paragraphs
   ========================================================================== */

p {
	margin: 0 0 mixins.$indent-var;

	// sibling indentation
  @if variables.$paragraph-indent == true {
    & + p {
			text-indent: mixins.$indent-var;
			margin-top: -(mixins.$indent-var);
		}
	}
}

/* 
   Code snippets
   ========================================================================== */

tt, code, kbd, samp, pre {
	font-family: variables.$code-font;
}

p,
li { 
	code {
		@include mixins.font-rem(12);

		line-height: 1.5;
		white-space: nowrap;
		margin: 0 2px;
		padding: 0 5px;
		border: 1px solid color.adjust(variables.$black, $lightness: 90%);
		background-color: #efefef;
		@include mixins.rounded(3px);
	}
}

pre {
	@include mixins.font-rem(12);

	line-height: 1.5;
	overflow-x: auto;
}
</file>

<file path="sumedhjoshi.github.io/_sass/variables.scss">
@use "sass:color";

/* ==========================================================================
   Sass variables
   ========================================================================== */

/* 
   Typography
   ========================================================================== */

$doc-font-size: 16;
$doc-line-height: 26;
$paragraph-indent: true !default;
$base-font: 'PT Serif', serif;
$heading-font: 'PT Sans Narrow', sans-serif;
$code-font: monaco, "Courier New", "DejaVu Sans Mono", "Bitstream Vera Sans Mono", monospace;
$alt-font: $base-font;

/*
   Colors
   ========================================================================== */

$bodycolor      : #fff;
$body-color     : $bodycolor;
$textcolor      : #333332;
$text-color     : $textcolor;
$border-color   : $text-color;
$basecolor 		  : #343434;	  
$compcolor 		  : spin($basecolor, 180); 
$bordercolor    : $basecolor;
$white          : #fff;
$black          : #111;
$accentcolor    : $black;

/* buttons */
$primary			: $black;
$success			: #5cb85c;
$warning			: #dd8338;
$danger				: #C64537;
$info				  : #308cbc;

/* brands */
$facebook-color     : #3b5998;
$flickr-color       : #ff0084;
$foursquare-color   : #0cbadf;
$google-plus-color  : #dd4b39;
$instagram-color    : #4e433c;
$linkedin-color     : #4875b4;
$pinterest-color    : #cb2027;
$rss-color          : #fa9b39;
$tumblr-color       : #2c4762;
$twitter-color      : #55acee;
$vimeo-color        : #1ab7ea;
$youtube-color      : #f33;

/* links */
$linkcolor 		: #343434;
