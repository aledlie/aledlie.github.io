<file path="sumedhjoshi.github.io/_sass/vendor/font-awesome/_spinning.scss">
@use "variables";

// Spinning Icons
// --------------------------

.#{variables.$fa-css-prefix}-spin {
  animation: spin 2s infinite linear;
  animation: spin 2s infinite linear;
  animation: spin 2s infinite linear;
  animation: spin 2s infinite linear;
}

@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(359deg); }
}
@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(359deg); }
}
@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(359deg); }
}
@keyframes spin {
  0% {
    transform: rotate(0deg);
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(359deg);
    transform: rotate(359deg);
  }
}
</file>

