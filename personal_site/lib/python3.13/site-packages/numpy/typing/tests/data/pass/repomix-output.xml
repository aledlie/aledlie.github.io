    np.get_printoptions()
    str(AR)

    np.array2string(
        AR,
        max_line_width=5,
        precision=2,
        suppress_small=True,
        separator=";",
        prefix="test",
        threshold=5,
        floatmode="fixed",
        suffix="?",
        legacy="1.13",
    )
    np.format_float_scientific(1, precision=5)
