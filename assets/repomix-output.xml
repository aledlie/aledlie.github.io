    }
  }

  .archive__item-excerpt {
    color: #666;
    font-size: 0.95em;
    line-height: 1.5;
    margin-bottom: 0.25rem;
  }

  .archive__item-date {
    color: #999;
    font-size: 0.85em;
  }
}

/* SINGLE POST - Enhanced typography */
.single {
  .page-title {
    font-size: 2.2em;
    font-weight: 700;
    margin-bottom: 1rem;
    line-height: 1.2;
  }

  .page-meta {
    color: #999;
    font-size: 0.9em;
    margin-bottom: 2rem;
    padding-bottom: 1rem;
    border-bottom: 1px solid #eee;
  }
}

/* FOOTER - Clean styling */
.page-footer {
  border-top: 1px solid #eee;
  margin-top: 3rem;
  padding: 2rem 0;
  text-align: center;

  .page-footer-copyright {
    color: #999;
    font-size: 0.85em;
  }
}

/* GLOBAL LINK OVERRIDES - Clean black text */
a {
  color: #333 !important;
  text-decoration: none !important;

  &:hover {
    color: #0066cc !important;
  }
}

/* Preserve navigation link styling */
.greedy-nav a {
  color: #666 !important;

  &:hover {
    color: #333 !important;
  }
}

/* SOCIAL SHARING */
.page-share {
  border-top: 1px solid #eee;
  padding-top: 1.5rem;
  margin-top: 2rem;

  .btn {
    margin-right: 0.5rem;
    margin-bottom: 0.5rem;
  }
}

/* RESPONSIVE DESIGN */
@media (max-width: 1024px) {
  #main {
    padding: 0.25rem;
  }

  .sidebar {
    padding: 0.25rem;
    text-align: center;

    .author-avatar img {
      width: 120px;
      height: 120px;
    }
  }

  .page-content {
    padding: 0 0.25rem;
  }
}

@media (max-width: 768px) {
  .greedy-nav {
    padding: 0.1rem 0.5rem;
    flex-direction: column;
    align-items: flex-start;

    .site-title {
      margin-right: 0;
      margin-bottom: 0.1rem;
      font-size: 1.2em;
    }

    .visible-links {
      gap: 0.02rem;

      a {
        padding: 0.1rem 0.2rem;
        font-size: 0.9em;
        font-weight: 900;
      }
    }
  }

  .single .page-title {
    font-size: 1.8em;
  }

  .archive .list__item .archive__item-title {
    font-size: 1.2em;
  }
}
